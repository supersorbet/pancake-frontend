{
  "name": "@pancakeswap/pools",
  "version": "0.0.1",
  "sideEffects": false,
  "private": true,
  "scripts": {
    "build": "yarn typechain && tsup",
    "test": "vitest --run",
    "typechain": "typechain --out-dir src/abis/types --target=ethers-v5 \"src/abis/*.json\"",
    "postinstall": "yarn typechain"
  },
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./abis/": "./src/abis/",
    "./package.json": "./package.json"
  },
  "peerDependencies": {
    "@pancakeswap/multicall": "*",
    "@pancakeswap/sdk": "*",
    "@pancakeswap/token-lists": "*",
    "@pancakeswap/tokens": "*",
    "@pancakeswap/utils": "*"
  },
  "dependencies": {
    "@ethersproject/bignumber": "^5.0.0",
    "@ethersproject/providers": "^5.0.0",
    "bignumber.js": "^9.0.0",
    "lodash": "^4.17.21"
  },
  "devDependencies": {
    "@pancakeswap/multicall": "*",
    "@pancakeswap/sdk": "*",
    "@pancakeswap/token-lists": "*",
    "@pancakeswap/tokens": "*",
    "@pancakeswap/utils": "*",
    "@typechain/ethers-v5": "^8.0.5",
    "tsconfig": "*",
    "tsup": "^6.6.3",
    "typechain": "^6.1.0",
    "typescript": "^4.9.4",
    "vitest": "^0.27.2"
  }
}
